{
  "name": "kakapo",
  "version": "0.1.1",
  "description": "Next generation mocking library in Javascript",
  "main": "src/kakapo.js",
  "scripts": {
    "start": "npm run serve | npm run dev",
    "dev": "webpack-dev-server --progress --colors --port 8090",
    "test": "./node_modules/karma/bin/karma start .karma.conf.js --single-run",
    "test:watch": "./node_modules/karma/bin/karma start .karma.conf.js",
    "test:browser": "npm-run-all -p test:browser:watch test:browser:server",
    "test:browser:watch": "watchify tests/tape-dom.js -o tests/browser/test_bundle.js -t [ babelify --presets [ es2015 ] ] -v",
    "test:browser:server": "serve ./tests/browser & open http://localhost:3000/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devlucky/Kakapo.js.git"
  },
  "author": "devlucky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devlucky/Kakapo.js/issues"
  },
  "homepage": "https://github.com/devlucky/Kakapo.js#readme",
  "devDependencies": {
    "babel-core": "^5.4.3",
    "babel-loader": "^5.1.2",
    "babel-preset-es2015": "6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "ghooks": "^1.2.0",
    "http-server": "~0.7.1",
    "karma": "0.13.22",
    "karma-browserify": "5.0.3",
    "karma-firefox-launcher": "0.1.7",
    "karma-tap": "1.0.4",
    "mocha-phantomjs": "^4.0.1",
    "npm-run-all": "1.7.0",
    "serve": "1.4.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "tape-dom": "0.0.10",
    "tape-run": "^2.1.3",
    "watchify": "^3.7.0",
    "webpack": "~1.4.4",
    "webpack-dev-server": "~1.6.5"
  },
  "dependencies": {
    "faker": "^3.1.0",
    "lodash": "4.10.0",
    "parse-url": "1.3.0",
    "path-match": "1.2.4",
    "path-to-regexp": "^1.2.1",
    "query-string": "4.1.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  }
}
